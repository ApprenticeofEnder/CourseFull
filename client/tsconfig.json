{
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "typeRoots": [
            "@coursefull",
            "node_modules",
            "node_modules/@types",
            "node_modules/@eslint"
        ],
        "types": [],
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@coursefull/*": ["./@coursefull/*"],
            "@coursefull": ["./@coursefull/index.d"],
            "@*": ["./*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "@coursefull/index.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "components/Disclosure",
        "app/auth/logout"
    ],
    "exclude": ["node_modules", "scripts"]
}
